{
  "name": "Send Text to Notion",
  "description": "Sends selected text to Notion for note-taking!",
  "version": "0.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
      "send-to-notion-default": {
      "suggested_key": {
        "default": "Ctrl+Shift+P",
        "mac": "Command+Shift+P"
      },
      "description": "send to notion's default notebook text-clippings"
    }
  },
  "permissions": [
    "storage", "activeTab", "contextMenus",
    "https://api.notion.com/**"
  ],
  "icons": {
    "16": "/images/get_started16.png",
    "32": "/images/get_started32.png",
    "48": "/images/get_started48.png",
    "128": "/images/get_started128.png"
  },
  "options_page": "options.html"
}
